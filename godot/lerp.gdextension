[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 =     "res://../gdextension/debug/librust_client.so"
linux.release.x86_64 =   "res://../gdextension/release/librust_client.so"
windows.debug.x86_64 =   "res://../gdextension/debug/rust_client.dll"
windows.release.x86_64 = "res://../gdextension/release/rust_client.dll"
macos.debug =            "res://../gdextension/debug/librust_client.dylib"
macos.release =          "res://../gdextension/release/librust_client.dylib"
macos.debug.arm64 =      "res://../gdextension/debug/librust_client.dylib"
macos.release.arm64 =    "res://../gdextension/release/librust_client.dylib"